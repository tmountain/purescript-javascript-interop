{
  "name": "ps-js-interop",
  "version": "0.0.1",
  "description": "Demonstrate PureScript to JavaScript interop.",
  "scripts": {
    "start": "parcel src/index.html --global index",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "spago install",
    "build": "spago bundle-module --main Main --to src/main.js",
    "build-production": "spago bundle-module --main Main --to src/main.js && parcel build src/index.html --global index",
    "clean": "rm -rf .cache .spago .psci_modules node_modules output dist"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "pulp": "^13.0.0",
    "purescript": "^0.13.0",
    "purty": "^4.3.0",
    "spago": "^0.8.5"
  }
}
